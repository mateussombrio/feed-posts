<div class="div-search-bar">
    <input type="text" placeholder="Search">
</div>

{{#each post}}
    <div class="div-post">
        <div class="div-user">
            <img src="assets/person.svg" alt="user-icon">
            <p>{{this.User.name}} {{this.User.lastname}}</p>
        </div>
        <p>{{this.description}}</p>
    </div>

{{/each}}
